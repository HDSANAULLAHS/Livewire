<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <!-- We only use the boldcondensed font so set that as both the normal and bold variant -->
    <font android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/dinpro_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        app:font="@font/dinpro_regular" />

    <font android:fontStyle="normal"
        android:fontWeight="700"
        android:font="@font/dinpro_condbold"
        app:fontStyle="normal"
        app:fontWeight="700"
        app:font="@font/dinpro_condbold" />

</font-family>
